<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<schulung>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:schulungen/locallang.xml:schulung</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.welcome>
						<TCEforms>
							<label>LLL:EXT:schulungen/locallang.xml:welcome</label>
							<config>
								<type>text</type>
								<cols>40</cols>
								<rows>15</rows>
								<eval>required</eval>
							</config>
							<defaultExtras>richtext[*]:rte_transform[flag=rte_enabled|mode=ts_css]</defaultExtras>
						</TCEforms>
					</settings.welcome>
					<settings.afterListing>
						<TCEforms>
							<label>LLL:EXT:schulungen/locallang.xml:afterListing</label>
							<config>
								<type>text</type>
								<cols>40</cols>
								<rows>15</rows>
								<eval>required</eval>
							</config>
							<defaultExtras>richtext[*]:rte_transform[flag=rte_enabled|mode=ts_css]</defaultExtras>
						</TCEforms>
					</settings.afterListing>
					<settings.contact>
						<TCEforms>
							<label>LLL:EXT:schulungen/locallang.xml:contact</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tt_address</allowed>
								<size>3</size>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</settings.contact>
				</el>
			</ROOT>
		</schulung>
	</sheets>
</T3DataStructure>